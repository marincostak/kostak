#include <stdio.h>
#include <math.h>
int main() {
int x,y,b,a;
printf("������ ����� x =");
scanf("%d",&x);
printf("������ ����� y =");
scanf("%d",&y);
printf("������ ����� b =");
scanf("%d",&b);
if(b < 0)
{
  a = b+sqrt(x-4*y);
  printf("���������� ����� �����\n%d",a);
}
if(b > 7)
{
  a = ((b*y)/(2*y-3*x))-4;
  printf("���������� ����� �����\n%d",a);
}
 if(b == 0)
{
  a = pow(b,2)-2*x*y+(4/(pow(b,2)-x*y));
  printf("���������� ����� �����\n%d",a);
} 
  return 0;
}
